{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "debug": {
            "format": "%(asctime)s [%(process)d] [%(thread)d] [%(levelname)s] %(name)s.%(funcName)s(): %(message)s",
            "formatTime": "%Y-%m-%d %H:%M:%S"
        },
        "verbose": {
            "format": "%(asctime)s [%(levelname)s] %(name)s.%(funcName)s(): %(message)s",
            "formatTime": "%Y-%m-%d %H:%M:%S"
        },
        "matlab": {
            "format": "[%(levelname)s] py.%(name)s.%(funcName)s(): %(message)s"
        },
        "simple": {
            "format": "[%(levelname)s] %(name)s: %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "NOTSET",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        },
        "console_debug": {
            "class": "logging.StreamHandler",
            "level": "NOTSET",
            "formatter": "debug",
            "stream": "ext://sys.stdout"
        },
        "matlab": {
            "class": "logging.StreamHandler",
            "level": "NOTSET",
            "formatter": "matlab",
            "stream": "ext://sys.stdout"
        },
        "info_file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "verbose",
            "filename": "info.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "encoding": "utf8",
            "mode": "w",
            "delay": true
        },
        "error_file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "ERROR",
            "formatter": "verbose",
            "filename": "errors.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "encoding": "utf8",
            "mode": "w",
            "delay": true
        }
    },
    "root": {
        "level": "NOTSET",
        "handlers": []
    }
}
